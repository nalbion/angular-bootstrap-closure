{
  "name": "angular-bootstrap-closure",
  "version": "0.0.3",

  "author": "Nicholas Albion",
  "description": "Bootstrap UI components for AngularJS apps using Closure Compiler, no jQuery dependencies",
  "keywords": [
    "angular", "angularjs", "bootstrap", "closure", "closure compiler",
    "slider"
  ],

  "homepage": "http://nalbion.github.io/angular-bootstrap-closure",
  "repository": {
    "type": "git",
    "url": "https://github.com/nalbion/angular-bootstrap-closure.git"
  },

  "ignore": [
    ".gitignore", ".tmp", ".idea", ".jshintrc",
    "gulpfile.js", "demo",
    "dist/abc.js.map", "dist/deps.js", "dist/index.html"
  ],

  "dependencies": {
    "bootstrap": "~3.3.1",
    "angularjs": "~1.3.9",
    "closure-library": "https://raw.githubusercontent.com/google/closure-library/master/closure/goog/base.js"
  },
  "devDependencies": {
    "closure-compiler": "http://dl.google.com/closure-compiler/compiler-latest.zip",
    "externs-angular": "https://raw.githubusercontent.com/google/closure-compiler/master/contrib/externs/angular-1.3.js",
    "externs-angular-q": "https://raw.githubusercontent.com/google/closure-compiler/master/contrib/externs/angular-1.3-q.js",
    "externs-angular-http": "https://raw.githubusercontent.com/google/closure-compiler/master/contrib/externs/angular-1.3-http-promise.js"
  }
}
